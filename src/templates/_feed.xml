 <?xml version="1.0" encoding="UTF-8"?>
 <rss xmlns:media="http://search.yahoo.com/mrss/" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:dc="http://purl.org/dc/elements/1.1/" version="2.0">
   <channel>
     <title>{{ settings['Render']['BlogTitle'] }}</title>
     <description>{{ settings['Render']['BlogDescription'] }}</description>
     <pubDate>{{ last_pub_date }}</pubDate>
     <link>{{ settings['Routes']['BaseUrl'] }}</link>
     <image>
       <link>{{ settings['Routes']['BaseUrl'] }}</link>
       <url>{{ settings['Routes']['BaseUrl'] }}/static/{{ settings['Routes']['Logo'] }}</url>
       <title>{{ settings['Render']['BlogTitle'] }}</title>
     </image>
     <atom:link href='{{ settings["Routes"]["BaseUrl"] }}/{{ settings["Routes"]["RssFeed"] }}' rel='self' type='application/rss+xml' />
     {% for post in posts %}
     <item>
       <title>{{ post.title }}</title>
       <link>{{ settings['Routes']['BaseUrl'] }}{{ post.full_url }}/</link>
       <description>{{ post.description }}</description>
       <pubDate>{{ post.date_rfc822 }}</pubDate>
       <guid isPermaLink="false">{{ settings['Routes']['BaseUrl'] }}{{ post.full_url }}/</guid>
     </item>
     {% endfor %}
   </channel>
 </rss>
